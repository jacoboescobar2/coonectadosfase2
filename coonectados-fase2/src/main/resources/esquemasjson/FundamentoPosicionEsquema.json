{
   "definitions": {},
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://example.com/root.json",
   "type": "array",
   "title": "The Root Schema",
   "items": {
      "$id": "#items",
      "type": "object",
      "title": "The Items Schema",
      "required": [
         "idPosicion",
         "timestamp",
         "nombrePosicion",
         "fecIniPosicion",
         "fecFinPosicion",
         "puestoPosicion",
         "lugTrabPosicion",
         "unidOrgPosicion",
         "nombreUnidOrg",
         "empresa",
         "idNegocio",
         "nombreNegocio",
         "idRegional",
         "claveAdmtva",
         "regAction"
      ],
      "properties": {
         "idPosicion": {
            "$id": "#items/properties/idPosicion",
            "type": "string",
            "title": "The Idposicion Schema",
            "examples": [
               "100036"
            ],
            "pattern": "^(.*)$",
            "minLength": 1,
            "maxLength": 10
         },
         "timestamp": {
            "$id": "#items/properties/timestamp",
            "type": "string",
            "title": "The Timestamp Schema",
            "examples": [
               "20191008164712353"
